{
  "name": "jarvis-web",
  "version": "0.1.0",
  "private": true,
  "productName": "Jarvis",
  "description": "Just A Rather Very Intelligent System (J.A.R.V.I.S.)",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "cross-env NODE_ENV=development eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "yarn --silent lint --fix; exit 0",
    "lint-styles": "stylelint --ignore-path .eslintignore '**/*.*(css|scss)' --syntax scss",
    "lint-styles-fix": "yarn --silent lint-styles --fix; exit 0"
  },
  "lint-staged": {
    ".src/*.{tsx,ts,js}": [
      "cross-env NODE_ENV=development eslint --cache --fix"
    ],
    "{*.json,.{eslintrc,prettierrc,stylelintrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "stylelint --ignore-path .eslintignore --syntax scss --fix",
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/Semantix/frontend/jarvis-web.git"
  },
  "author": {
